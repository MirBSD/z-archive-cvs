#include <stdio.h>
#include <stdlib.h>

int 
main ()
{
  int major, minor, micro;
  char *tmp_version;

  system ("touch conf.glibtest");

  /* HP/UX 9 (%@@#!) writes to sscanf strings */
  tmp_version = g_strdup("$min_glib_version");

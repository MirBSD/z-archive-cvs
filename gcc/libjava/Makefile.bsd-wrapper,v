@COPTS, CXXOPTS for build
@
text
@d1 1
a1 1
# $MirOS: gcc/libjava/Makefile.bsd-wrapper,v 1.18 2005/10/06 22:22:00 tg Exp $
a55 12
.if ${DEBUGPROGS:L} == "yes"
.  for _i in ${INSTPROGS}
	-rm -f ${DESTDIR}/usr/bin/${_i}.dbg
	cd ${DESTDIR}/usr/bin && \
	    chmod 0600 ${_i} && \
	    objcopy --only-keep-debug ${_i} ${_i}.dbg && \
	    objcopy --strip-debug --add-gnu-debuglink=${_i}.dbg ${_i} && \
	    chmod ${BINMODE} ${_i} && \
	    chmod ${SHAREMODE} ${_i}.dbg && \
	    chown ${BINOWN}:${BINGRP} ${_i} ${_i}.dbg
.  endfor
.endif
@



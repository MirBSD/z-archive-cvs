/* Specify predefined symbols in preprocessor.  */

#define TARGET_OS_CPP_BUILTINS()		\
  do						\
    {						\
	builtin_define ("__rtems__");		\
	builtin_define ("__USE_INIT_FINI__");	\
	builtin_assert ("system=rtems");	\
	if (!TARGET_80387)			\
	  builtin_define ("_SOFT_FLOAT");	\
    }						\
  while (0)
@



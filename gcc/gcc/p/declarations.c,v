a2043 15
static long
visible_list_length (tree t);

static long
visible_list_length (tree t)
{
  long res = 0;
  while (t)
    {
      res += !PASCAL_FIELD_SHADOWED (t);
      t = TREE_CHAIN (t);
    }
  return res;
}


since imake.port.mk would be too late, this means touching â€™em all

XXX these openbsd derived ports Makefiles surely look ugly
@
text
@# $MirOS: ports/graphics/xbmbrowser/Makefile,v 1.4 2007/04/01 11:43:48 bsiegert Exp $
# $OpenBSD: Makefile,v 1.13 2002/10/27 00:03:17 naddy Exp $
# $FreeBSD: Makefile,v 1.3 1997/08/26 07:12:20 tg Exp $

ONLY_FOR_PLATFORM=	MirBSD:*:* OpenBSD:*:*

COMMENT=	view complete directories of X bitmaps and X pixmaps

DISTNAME=	xbmbrowser5.1
PKGNAME=	xbmbrowser-5.1-0
CATEGORIES=	graphics

MASTER_SITES=	${MASTER_SITE_XCONTRIB:=utilities/}

PERMIT_PACKAGE_CDROM=	Yes
PERMIT_PACKAGE_FTP=	Yes
PERMIT_DISTFILES_CDROM=	Yes
PERMIT_DISTFILES_FTP=	Yes

USE_X11=	Yes
CONFIGURE_STYLE=imake

NO_REGRESS=	Yes

.include <bsd.port.mk>
@



a10 1
FULLPKGNAME=		rar-${V}-1-linux
a37 7

.if ${SYSCONFDIR} != "/etc"
do-configure:
	@@echo "ERROR: \$$SYSCONFDIR is not set to /etc"
	@@echo "The binary will not be able to find rarfiles.lst!"
	@@exit 1
.endif
@



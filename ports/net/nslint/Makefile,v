FLAVOUR?=

.if ${FLAVOUR:L} == "static"
CONFIGURE_ENV+=		LDFLAGS=-static
.endif

CONFIGURE_STYLE=	gnu
ALL_TARGET=		nslint
INSTALL_TARGET=		install install-man

NO_REGRESS=		Yes

.include <bsd.port.mk>
@



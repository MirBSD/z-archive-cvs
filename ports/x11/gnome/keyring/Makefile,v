1.6
log
@Update gnome-keyring to 2.18.1
@
text
@# $MirOS: ports/x11/gnome/keyring/Makefile,v 1.5 2007/04/18 20:05:05 bsiegert Exp $
# $OpenBSD: Makefile,v 1.4 2004/12/15 13:57:56 alek Exp $

COMMENT=		password agent for the GNOME project

DISTNAME=		gnome-keyring-0.8.1
CATEGORIES=		x11/gnome

HOMEPAGE=		http://www.gnome.org/

# LGPL
PERMIT_PACKAGE_CDROM=	Yes
PERMIT_PACKAGE_FTP=	Yes
PERMIT_DISTFILES_CDROM=	Yes
PERMIT_DISTFILES_FTP=	Yes

MASTER_SITES=		${MASTER_SITE_GNOME:=sources/gnome-keyring/0.8/}
EXTRACT_SUFX=		.tar.bz2

MODULES=		devel/gettext devel/pkgconfig

LIB_DEPENDS+=		gdk-x11-2.0.0.0,gdk_pixbuf-2.0.0.0,gtk-x11-2.0.0.0::x11/gtk+
LIB_DEPENDS+=		pangocairo-1.0.0.0::devel/pango
LIB_DEPENDS+=		dbus-1::sysutils/dbus
B_R_DEPENDS+=		::textproc/p5-XML-Parser

USE_X11=		Yes
USE_GMAKE=		Yes
CONFIGURE_STYLE=	autoconf
AUTOCONF_NEW=		Yes
CONFIGURE_ARGS+=	--disable-gtk-doc

.include <bsd.port.mk>
@



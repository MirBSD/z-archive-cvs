+        prog XPlay_CD xplaycd xplaycd
+        prog XM_ixer xmixer xmixer
+        prog XMMS xmms xmms
+	prog "Audacious Media Player" audacious audacious
+        prog Mplayer gmplayer gmplayer
+    }

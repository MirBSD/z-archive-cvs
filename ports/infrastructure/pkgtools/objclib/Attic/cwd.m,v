#include <stdio.h>
#include <string.h>
#include "objclib.h"
#include "parser.h"

@@implementation plistCwd

- (char *)toString
{
	char *tmp;
	
	asprintf(&tmp, "@@cwd %s", name);
	return tmp;
}

@@end

plistElement *parseCwd(char *command)
{
	return [[plistCwd alloc] initWithName: command + 5];
}
@



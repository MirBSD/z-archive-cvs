RUN_DEPENDS=	${BUILD_DEPENDS}

USE_X11=	Yes
NO_BUILD=	Yes
NO_REGRESS=	Yes

do-install:
	cd $(WRKSRC)/tkcvs; ./doinstall

.include <bsd.port.mk>
@



LinkSourceFile(ftgzip.h,$(FREETYPEINC))
LinkSourceFile(ftimage.h,$(FREETYPEINC))
LinkSourceFile(ftincrem.h,$(FREETYPEINC))
LinkSourceFile(ftlcdfil.h,$(FREETYPEINC))

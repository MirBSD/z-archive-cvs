@XCOMM $XFree86: xc/lib/freetype2/freetype/cache/Imakefile,v 1.4 2004/04/26 16:15:56 dawes Exp $

FREETYPETOP = $(TOP)/extras/freetype2
FREETYPESRC = $(FREETYPETOP)/src
FREETYPEINC = $(FREETYPETOP)/include/freetype

HEADERS= \
        ftccache.h \
        ftccmap.h \
        ftcglyph.h \
        ftcimage.h \
        ftcmanag.h \
        ftcmru.h \
        ftcsbits.h


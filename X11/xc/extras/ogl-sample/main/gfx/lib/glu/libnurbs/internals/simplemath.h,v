/* $XFree86: xc/extras/ogl-sample/main/gfx/lib/glu/libnurbs/internals/simplemath.h,v 1.3 2004/04/03 22:26:20 dawes Exp $ */

/*
 * simplemath.h
 *
 * Header: //depot/main/gfx/lib/glu/libnurbs/internals/simplemath.h#4 $
 */

#ifndef __glusimplemath_h_
#define __glusimplemath_h_

/* simple inline routines */

inline int 
max( int x, int y ) { return ( x < y ) ? y : x; }

inline REAL 
min( REAL x, REAL y ) { return ( x > y ) ? y : x; }

#undef abs

#if defined(_SCO_DS) || defined(__SCO__)
#define abs __gluabs
#endif
inline REAL 
abs( REAL x ) { return ( x < 0.0 ) ? -x : x; }

#endif /* __glusimplemath_h_ */
@



# fully.

SubDir  FT2_TOP $(FT2_SRC_DIR) psnames ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = psmodule
               pspic
               ;
  }
  else
  {
    _sources = psnames ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/psnames Jamfile
@



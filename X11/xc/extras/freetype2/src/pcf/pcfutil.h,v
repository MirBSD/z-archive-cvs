#define PCFUTIL_H_


#include <ft2build.h>
#include FT_CONFIG_CONFIG_H


FT_BEGIN_HEADER

  FT_LOCAL( void )
  BitOrderInvert( unsigned char*  buf,
                  size_t          nbytes );

  FT_LOCAL( void )
  TwoByteSwap( unsigned char*  buf,
               size_t          nbytes );

  FT_LOCAL( void )
  FourByteSwap( unsigned char*  buf,
                size_t          nbytes );

FT_END_HEADER

#endif /* PCFUTIL_H_ */


/* END */

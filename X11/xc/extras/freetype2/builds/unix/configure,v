int
main ()
{


        Str255     familyName;
        SInt16     familyID   = 0;
        FMInput*   fmIn       = NULL;
        FMOutput*  fmOut      = NULL;


        GetFontName( familyID, familyName );
        GetFNum( familyName, &familyID );
        fmOut = FMSwapFont( fmIn );


  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"; then :

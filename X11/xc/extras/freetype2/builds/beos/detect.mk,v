.PHONY: setup


ifeq ($(PLATFORM),ansi)

  ifdef BE_HOST_CPU

    PLATFORM := beos

  endif # test MACHTYPE beos
endif

ifeq ($(PLATFORM),beos)

  DELETE      := rm -f
  CAT         := cat
  SEP         := /
  BUILD_DIR   := $(TOP_DIR)/builds/beos
  CONFIG_FILE := beos.mk

  setup: std_setup

endif   # test PLATFORM beos


# EOF
@



#include <stdio.h>
#include <string.h>
#include "stubs.h"

/*
 * Implementation
 */

#ifdef __UNIXOS2__
static char dummy;
#endif

#if !defined(USE_MODULES)
/* these are defined in libdummy.a */
int
ErrorF(const char *fmt, ...)
{
    int retval;
    va_list ap;

    va_start(ap, fmt);
    retval = vfprintf(stderr, fmt, ap);

    va_end(ap);

    return (retval);
}

int
VErrorF(const char *fmt, va_list ap)
{
    int retval;

    retval = vfprintf(stderr, fmt, ap);

    return (retval);
}

#endif /* !defined(USE_MODULES) */

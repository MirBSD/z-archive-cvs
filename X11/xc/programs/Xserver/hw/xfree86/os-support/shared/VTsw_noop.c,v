#include "X.h"

#include "xf86.h"
#include "xf86Priv.h"
#include "xf86_OSlib.h"

/*
 * No-op functions for OSs without VTs
 */

Bool
xf86VTSwitchPending()
{
	return(FALSE);
}

Bool
xf86VTSwitchAway()
{
	return(FALSE);
}

Bool
xf86VTSwitchTo()
{
	return(TRUE);
}

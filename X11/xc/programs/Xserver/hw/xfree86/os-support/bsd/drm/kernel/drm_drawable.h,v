#include "drmP.h"

int DRM(adddraw)( DRM_IOCTL_ARGS )
{
	drm_draw_t draw;

	draw.handle = 0;	/* NOOP */
	DRM_DEBUG("%d\n", draw.handle);
	
	DRM_COPY_TO_USER_IOCTL( (drm_draw_t *)data, draw, sizeof(draw) );

	return 0;
}

int DRM(rmdraw)( DRM_IOCTL_ARGS )
{
	return 0;		/* NOOP */
}

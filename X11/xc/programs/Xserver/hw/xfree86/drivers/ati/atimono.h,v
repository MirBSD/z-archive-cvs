#ifndef ___ATIMONO_H___
#define ___ATIMONO_H___ 1

#ifndef BIT_PLANE
#   define BIT_PLANE 3
#endif

#ifndef MONO_BLACK
#   define MONO_BLACK 0x00U
#endif

#ifndef MONO_WHITE
#   define MONO_WHITE 0x3FU
#endif

#ifndef MONO_OVERSCAN
#   define MONO_OVERSCAN 0x01U
#endif

#endif /* ___ATIMONO_H___ */

		if ( eval $LINKIT $* >/dev/null 2>/dev/null )
d18 1
a18 1
			LINKIT="$LINKIT $OPT"
d22 1
a22 1
		LINKIT="$LINKIT $OPT"
d26 1
a26 1
eval $LINKIT
@



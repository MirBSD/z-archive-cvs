1.3
log
@bump patchlevel; allow cpp change
@
text
@#!/bin/sh
# $MirOS: src/libexec/cpp/xcpp.sh,v 1.2 2005/07/07 13:39:33 tg Exp $

if [ -e /usr/bin/mcpp ]; then
	exec /usr/bin/mcpp "$@@"
fi

if [ -e /usr/libexec/cpp ]; then
	exec /usr/libexec/cpp "$@@"
fi

exec mcpp "$@@"

echo exec of cpp failed: $?
exit 1
@



CFLAGS+=-Wall -Wno-unused
LDADD+=	-lutil
DPADD+=	${LIBUTIL}

BINOWN=	root
BINGRP= auth
BINMODE=4555
BINDIR= /usr/libexec/auth

.include <bsd.prog.mk>
@



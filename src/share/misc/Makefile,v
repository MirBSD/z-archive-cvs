1.18
log
@add useful pinouts: 40-pin IDE (“PATA”) for now, plus plip(4)
@
text
@# $MirOS: src/share/misc/Makefile,v 1.17 2019/08/16 21:52:50 tg Exp $
# $OpenBSD: Makefile,v 1.10 2004/11/19 18:18:53 deraadt Exp $
# from: @@(#)Makefile	5.13 (Berkeley) 5/7/91

NOOBJ=	yes
FILES=
FILES+=	acronyms
FILES+=	ascii
FILES+=	birthtoken
FILES+=	countrycodes
FILES+=	eqnchar
FILES+=	getopt
FILES+=	inter.phone
FILES+=	licence.template
FILES+=	man.template
FILES+=	mdoc.template
FILES+=	na.phone
FILES+=	operator
FILES+=	pinouts
FILES+=	scsi_modes
FILES+=	usb_hid_usages
FILES+=	utf-8-demo
FILES+=	zipcodes

realinstall:
	cd ${.CURDIR}; install -c -o ${BINOWN} -g ${BINGRP} -m ${SHAREMODE} \
	    ${FILES} ${DESTDIR}${BINDIR}/misc/

resort:
	cd ${BSDSRCDIR:Q}/usr.bin/wtf && ${MAKE} __CRAZY=Yes sortdb && \
	    obj/sortdb ${.CURDIR:Q}/acronyms >${.CURDIR:Q}/acronyms.sorted
	@@cd ${.CURDIR:Q} && ls -l acronyms acronyms.sorted >&2
	@@echo N: mv acronyms.sorted acronyms \# manually, then commit that >&2

.include <bsd.prog.mk>
@



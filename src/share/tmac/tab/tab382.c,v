#define INCH 240
/*
 * DTC 382 10 pitch
 * nroff driving tables
 * width and code tables
 */

struct {
	int bset;
	int breset;
	int Hor;
	int Vert;
	int Newline;
	int Char;
	int Em;
	int Halfline;
	int Adj;
	const char *twinit;
	const char *twrest;
	const char *twnl;
	const char *hlr;
	const char *hlf;
	const char *flr;
	const char *bdon;
	const char *bdoff;
	const char *ploton;
	const char *plotoff;
	const char *up;
	const char *down;
	const char *right;
	const char *left;
	const char *codetab[256-32];
	int zzz;
	} t = {
/*bset*/	0,
/*breset*/	0177420,
/*Hor*/		INCH/60,
/*Vert*/	INCH/48,
/*Newline*/	INCH/6,
/*Char*/	INCH/10,
/*Em*/		INCH/10,
/*Halfline*/	INCH/12,
/*Adj*/		INCH/10,
/*twinit*/	"\033\006",
/*twrest*/	"\033\006",
/*twnl*/	"\r\n",
/*hlr*/		"\033H",
/*hlf*/		"\033h",
/*flr*/		"\032",
/*bdon*/	"\033Z",
/*bdoff*/	"\033z",
/*ploton*/	"\006",
/*plotoff*/	"\033\006",
/*up*/		"\032",
/*down*/	"\n",
/*right*/	" ",
/*left*/	"\b",
/*codetab*/	{
#include "code.300"
		},
/*zzz*/		0
};
@



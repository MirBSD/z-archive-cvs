@d39 1
a39 10
.ie \\n(.$>8 .Dd "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6" "\\$7" "\\$8" "\\$9"
.el .ie \\n(.$>7 .Dd "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6" "\\$7" "\\$8"
.el .ie \\n(.$>6 .Dd "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6" "\\$7"
.el .ie \\n(.$>5 .Dd "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6"
.el .ie \\n(.$>4 .Dd "\\$1" "\\$2" "\\$3" "\\$4" "\\$5"
.el .ie \\n(.$>3 .Dd "\\$1" "\\$2" "\\$3" "\\$4"
.el .ie \\n(.$>2 .Dd "\\$1" "\\$2" "\\$3"
.el .ie \\n(.$>1 .Dd "\\$1" "\\$2"
.el .ie \\n(.$>0 .Dd "\\$1"
.el .Dd
d45 1
a45 10
.ie \\n(.$>8 .TH "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6" "\\$7" "\\$8" "\\$9"
.el .ie \\n(.$>7 .TH "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6" "\\$7" "\\$8"
.el .ie \\n(.$>6 .TH "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6" "\\$7"
.el .ie \\n(.$>5 .TH "\\$1" "\\$2" "\\$3" "\\$4" "\\$5" "\\$6"
.el .ie \\n(.$>4 .TH "\\$1" "\\$2" "\\$3" "\\$4" "\\$5"
.el .ie \\n(.$>3 .TH "\\$1" "\\$2" "\\$3" "\\$4"
.el .ie \\n(.$>2 .TH "\\$1" "\\$2" "\\$3"
.el .ie \\n(.$>1 .TH "\\$1" "\\$2"
.el .ie \\n(.$>0 .TH "\\$1"
.el .TH
@



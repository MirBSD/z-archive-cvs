.	ie \\n(.$==0 .tm "Usage: .Sm [off | on]" (#\\n(.c)
.	el \{\
.		ds mN Sm
.		aV \\$1 \\$2 \\$3 \\$4 \\$5 \\$6 \\$7 \\$8 \\$9
.	\}

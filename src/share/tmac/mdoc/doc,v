.\" NS eO macro - enclose string open
.\" NS oM register (extension possible)
.de eO
.nr oM \\n(oM+1
.\" .tm eO last arg==A[\\n(aC]==\\*(A\\n(aC; aP==\\n(aP; oM==\\n(oM; dZ==\\n(dZ;
.ie \\n(aC==0 \{\
.	ie \\n(.$>0 \{\
.		aV \\$1 \\$2 \\$3 \\$4 \\$5 \\$6 \\$7 \\$8 \\$9
.		as b1 \\*(qL
.	\}
.	el \{\
.		as b1 \\*(qL
.		if (\\n(dZ==0)&(\\n(sM==1) \{\
.			nr dZ \\n(dZ+1
.			ds b2 \\*(b1
.			ds b1
.			nr lK \\n(.c
.			ev 2
.			fi
.			di eB
.		\}
.	\}
.\}
.el \{\
.	as b1 \\*(qL
.\}
.ie \\n(aC>0 \{\
.	if \\n(aC>\\n(aP \{\
.		nr aP \\n(aP+1
.		ie \\n(C\\n(aP==1 .\\*(A\\n(aP
.		el \{\
.			nr aP \\n(aP-1
.			No
.		\}
.	\}
.	if \\n(aC==\\n(aP \{\
.		if \\n(tP==1 \{\
.\" .tm SETTING Xt!!!
.			nr Xt 1
.		\}
.\".\" .		ds S0
.\"CHANGED		ds S0 \\*(iV
.		aY
.	\}
.\}
.el \{\
.	if \\n(oM>1 .as b1 \\*(sV
.\}
..
.\"
.\" NS eC macro - enclose string close
.\" NS aa local register
.de eC
.nr oM \\n(oM-1
.\" tm eC last arg==A[\\n(aC]==\\*(A\\n(aC; aP==\\n(aP; oM==\\n(oM; dZ==\\n(dZ;
.as b1 \\*(qR
.if \\n(aC==0 \{\
.	ie \\n(.$>0 \{\
.		aV \\$1 \\$2 \\$3 \\$4 \\$5 \\$6 \\$7 \\$8 \\$9
.	\}
.	el \{\
.		ie "\\*(xB"" \{\
.			pB
.		\}
.		el \{\
.			pB
.\\*(L\\n(lC
.			nr Xt 0
.			ds xB
.		\}
.	\}
.\}
.if \\n(aC>0 \{\
.	ie \\n(aC==\\n(aP \{\
.		ie \\n(oM==0 \{\
.			aZ
.		\}
.		el .aY
.	\}
.	el \{\
.		nr aa \\n(aP+1
.		if \\n(C\\n(aa==2 .as b1 \\*(S\\n(aC
.\" tm CURRENT arg (aP==\\*(A\\n(aP and ap+1==\\*(A\\n(aa) tP==\\n(tP Xt==\\n(Xt
.		rr aa
.		if \\n(tP>0 \{\
.\" tm UNSETTING Xt==\\n(Xt!!!!
.			if \\n(Xt>0 .nr Xt \\n(Xt-1
.\" tm NOW Xt==\\n(Xt!!!!
.		\}
.		No
.	\}
.\}
..

.	nr aP \\n(aP+1
.	if "\\*(A\\n(aP"on" \{\
.		ds sV \\*(iV
.		nr sM 1
.	\}
.	if "\\*(A\\n(aP"off" \{\
.		ds sV
.		rm S0 S1 S2 S3 S4 S5 S6 S7 S8 S9
.		nr sM 0
.	\}
.	ie \\n(aC>\\n(aP \{\
.		No
.	\}
.	el .aY
.\}

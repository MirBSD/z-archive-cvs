.\" NS Nm macro - Name of command or page topic
.\" NS n1 string - save first invocation of .Nm
.\" NS iS register - indent second command line in a synopsis
.de Nm
.if \\n(aC==0 \{\
.	ie \\n(.$==0 \{\
.		ie "\\*(n1"" .tm Usage: .Nm Name(s) ... \\*(Pu (#\\n(.c)
.		el \{\
.			ds mN Nm
.			ds A1 \\*(n1
.			nr fV 1
.			fV
.		\}
.	\}
.	el \{\
.		ds mN Nm
.		sW "\\$1"
.		aT "\\$1"
.		ie (\\n(aT==3):(\\n(aT==4) \{\
.			ds A1 \\*(n1
.			ds A2 \\$1
.			ds A3 \\$2
.			ds A4 \\$3
.			ds A5 \\$4
.			ds A6 \\$5
.			ds A7 \\$6
.			ds A8 \\$7
.			ds A9 \\$8
.			if !"\\$9"" .as A9 " \\$9
.			nr fV \\n(.$
.			if \\n(fV<9 .nr fV \\n(fV+1
.		\}
.		el \{\
.			ie "\\$1"" .ds A1 \\*(n1
.			el .ds A1 \\$1
.			ds A2 \\$2
.			ds A3 \\$3
.			ds A4 \\$4
.			ds A5 \\$5
.			ds A6 \\$6
.			ds A7 \\$7
.			ds A8 \\$8
.			ds A9 \\$9
.			nr fV \\n(.$
.		\}
.		fV
.	\}
.\}
.if \\n(aC>0 \{\
.	ie \\n(aC==\\n(aP \{\
.		as b1 \&\\*(nM\\*(n1\fP\s0
.		aZ
.	\}
.	el \{\
.		as b1 \\*(nM
.		nr aP \\n(aP+1
.		ie \\n(C\\n(aP==1 \{\
.			as b1 \&\\*(n1\fP\s0
.			\\*(A\\n(aP
.		\}
.		el \{\
.			nr cF \\n(.f
.			nr cZ \\n(.s
.			if \\n(nS \{\
.				if "\\*(mN"Nm" \{\
.					rs
.					in -\\n(iSu
.					ie \\n(nS>1 .br
.					el \{\
.						if \\n(iS==0 \{\
.							sw \\$1
.					nr iS ((\\n(sWu+1)*\\n(fW)u
.						\}
.					\}
.					in +\\n(iSu
.					ti -\\n(iSu
.					nr nS \\n(nS+1
.				\}
.			\}
.			if "\\*(n1"" .ds n1 \\*(A\\n(aP
.			nR
.		\}
.	\}
.\}
..

1.3
log
@merge all of src/share
@
text
@# $MirOS: src/share/doc/smm/Makefile,v 1.2 2005/03/06 19:38:27 tg Exp $
# $OpenBSD: Makefile,v 1.7 2004/10/10 02:44:18 mickey Exp $
# from: @@(#)Makefile	8.1 (Berkeley) 6/10/93

INSTDIR=smm
FILES=	00.contents Makefile Title
SUBDIR!=echo */Makefile | sed 's,/Makefile[[:>:]],,g'

.include <bsd.own.mk>

all: _SUBDIRUSE
all: Title.txt contents.txt

Title.ps: ${FILES}
	${ROFF} Title >${.TARGET}
Title.txt: ${FILES}
	${NROFF} Title >${.TARGET}

contents.ps: ${FILES}
	${ROFF} -ms 00.contents >${.TARGET}
contents.txt: ${FILES}
	${NROFF} -ms 00.contents >${.TARGET}

beforeinstall:
	install -c -o ${DOCOWN} -g ${DOCGRP} -m ${DOCMODE} ${FILES} \
	    ${DESTDIR}${DOCDIR}/${INSTDIR}

.include <bsd.subdir.mk>
@



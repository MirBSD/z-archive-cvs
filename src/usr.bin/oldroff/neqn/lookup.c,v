#include <sys/cdefs.h>
__SCCSID("@@(#)lookup.c	4.3 (Berkeley) 4/17/91");
__RCSID("$MirOS: src/usr.bin/oldroff/neqn/lookup.c,v 1.2 2007/04/28 00:12:50 tg Exp $");

#include <stdlib.h>

#include "e.h"
#include "ey.h"

#define	TBLSIZE	100

tbl	*keytbl[TBLSIZE];	/* key words */
tbl	*restbl[TBLSIZE];	/* reserved words */
tbl	*deftbl[TBLSIZE];	/* user-defined names */

struct {
	char	*key;
	int	keyval;
} keyword[]	={
	"sub", 	SUB,
	"sup", 	SUP,
	".EN", 	EOF,
	"from", 	FROM,
	"to", 	TO,
	"sum", 	SUM,
	"hat", 	HAT,
	"vec", VEC,
	"dyad", DYAD,
	"dot", 	DOT,

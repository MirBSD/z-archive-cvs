#include <sys/types.h>
#include <stdio.h>

#include "top.h"
#include "patchlevel.h"

char *
version_string(void)
{
	static char version[16];

	snprintf(version, sizeof(version), "%d.%d", VERSION, PATCHLEVEL);
	return(version);
}

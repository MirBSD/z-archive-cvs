#include <stdio.h>

int
main(int argc, char *argv[])
{
	int i, j, k;

	if (argc > 1)
		printf("%s\n",argv[1]);
	k = 0;
	j = 0;
	while((i = getchar()) != EOF) {
		if(k++) {
			putchar(',');
			j++;
		}
		if (j > 70) {
			putchar('\n');
			j = 0;
		}

		printf("%d", i);

		if (i > 99)
			j += 3;
		else if (i > 9)
			j += 2;
		else
			j++;
	}
	putchar('\n');
	if (argc > 2)
		printf("%s\n", argv[2]);
	return 0;
}
@



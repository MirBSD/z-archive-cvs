#include	"lstInt.h"
#include	<stddef.h>

/* Add datum to the end of a list only if it wasn't there already.
 * Returns false if datum was already there.
 */
bool
Lst_AddNew(Lst l, void *d)
{
    if (Lst_Member(l, d) != NULL)
	return false;
    else {
	Lst_AtEnd(l, d);
	return true;
    }
}


@/**	$MirOS: src/usr.bin/make/generate.c,v 1.5 2005/11/24 13:20:33 tg Exp $ */
/*	$OpenPackages$ */
/*	$OpenBSD: generate.c,v 1.6 2006/01/20 23:10:19 espie Exp $ */

/*

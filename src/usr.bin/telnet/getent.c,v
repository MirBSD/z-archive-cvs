#ifndef lint
/* from: static char sccsid[] = "@@(#)getent.c	8.2 (Berkeley) 12/15/93"; */
/* from: static char *rcsid = "$NetBSD: getent.c,v 1.5 1996/02/24 01:15:22 jtk Exp $"; */
static char *rcsid = "$OpenBSD: getent.c,v 1.2 2003/06/03 02:56:18 millert Exp $";
#endif /* not lint */

#include <stdlib.h>
#include "misc-proto.h"

static char *area;

int gtgetent(char *, char *);
char *gtgetstr(char *, char **);

/*ARGSUSED*/
int
gtgetent(cp, name)
char *cp, *name;
{
#ifdef	HAS_CGETENT
	char *dba[2];

	dba[0] = "/etc/gettytab";
	dba[1] = 0;
	return((cgetent(&area, dba, name) == 0) ? 1 : 0);
#else
	return(0);
#endif
}

#ifndef	SOLARIS
/*ARGSUSED*/
char *
gtgetstr(id, cpp)
char *id, **cpp;
{
# ifdef	HAS_CGETENT
	char *answer;
	return((cgetstr(area, id, &answer) > 0) ? answer : 0);
# else
	return(0);
# endif
}
#endif
@


1.1.1.1
log
@Import the OpenBSD foundation of MirOS BSD

#ifndef lint
#if 0
static char sccsid[] = "@@(#)cmdtab.c	8.1 (Berkeley) 6/6/93";
#endif
static const char rcsid[] = "$OpenBSD: cmdtab.c,v 1.5 2003/06/03 02:56:18 millert Exp $";

#include <machine/asm.h>

RCSID("$MirOS: src/kern/c/i386/memcmp.S,v 1.1 2008/08/01 18:09:20 tg Exp $")

	.intel_syntax noprefix
	.text

NENTRY(bcmp)
ENTRY(memcmp)
	push	esi
	push	edi
	mov	edi,12[esp]
	mov	esi,16[esp]
	mov	ecx,20[esp]
	xor	eax,eax		/* also ensures ZF is set, for if ECX == 0 */
	cld
	repe	cmpsb
	jne	Lnotf
	pop	edi
	pop	esi
	ret
Lnotf:	mov	al,[edi-1]
	movzx	edx,byte ptr [esi-1]
	sub	eax,edx
	pop	edi
	pop	esi
	ret
@



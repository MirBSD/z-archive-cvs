#include <libckern.h>

__RCSID("$MirOS: src/kern/c/memset.c,v 1.1 2008/08/01 18:09:17 tg Exp $");

void *
memset(void *b, int c, size_t len)
{
	uint8_t *cp = b;

	while (len--)
		*cp++ = (uint8_t)c;
	return (b);
}
@



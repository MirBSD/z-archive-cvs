MAINTAINER_MODE_FALSE!$MAINTAINER_MODE_FALSE$ac_delim
MAINT!$MAINT$ac_delim
HDEFINES!$HDEFINES$ac_delim
CC_FOR_BUILD!$CC_FOR_BUILD$ac_delim

test -z "$CONFIG_HEADERS" || echo timestamp > stamp-h
d23756 1
a23756 3
case "x$CONFIG_FILES" in
*) sed -e '/POTFILES =/r po/POTFILES' po/Makefile.in > po/Makefile ;;

FSFSRC=		${GNU_BINUTILS_AS}
MAN=		doc/as.1
ALL_TARGET=	all info
INST_TARGET=	INSTALL_MAN= install
CLEANFILES+=	doc/as.1 doc/gasver.texi

${MAN}: do-build

.include <bsd.cfwrap.mk>
@



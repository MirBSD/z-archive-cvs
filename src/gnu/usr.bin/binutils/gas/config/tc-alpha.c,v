/* Make register usage like in normal procedure call.
   Don't clobber PV and RA.  */
d3068 1
a3068 6
static void
emit_division (const expressionS *tok,
	       int ntok,
	       const void * symname)
{
  /* DIVISION and MODULUS. Yech.
d3070 6
a3075 8
     Convert
        OP x,y,result
     to
        mov x,R16	# if x != R16
        mov y,R17	# if y != R17
        lda AT,__OP
        jsr AT,(AT),0
        mov R0,result
d3077 6
a3082 2

#include "bfd.h"
#include "sysdep.h"
#include "libbfd.h"

static const bfd_arch_info_type arch_info_struct[] =
{
  /* hack alert: old old machines are really 16 and 24 bit arch ...  */
  {
    32, /* 32 bits in a word */
    32, /* 32 bits in an address */
    8,  /* 8 bits in a byte */
    bfd_arch_i370,
    360, /* for the 360 */
    "i370",
    "i370:360",
    3,
    FALSE, /* not the default */
    bfd_default_compatible,
    bfd_default_scan,
    &arch_info_struct[1]
  },
  {
    32, /* 32 bits in a word */
    32, /* 32 bits in an address */
    8,  /* 8 bits in a byte */
    bfd_arch_i370,
    370, /* for the 370 */
    "i370",
    "i370:370",
    3,
    FALSE, /* not the default */
    bfd_default_compatible,
    bfd_default_scan,
    0
  },
};

const bfd_arch_info_type bfd_i370_arch =
  {
    32, /* 32 bits in a word */
    32, /* 32 bits in an address */
    8,  /* 8 bits in a byte */
    bfd_arch_i370,
    0,  /* for the 360/370 common architecture */
    "i370",
    "i370:common",
    3,
    TRUE, /* the default */
    bfd_default_compatible,
    bfd_default_scan,
    &arch_info_struct[0]
  };
@



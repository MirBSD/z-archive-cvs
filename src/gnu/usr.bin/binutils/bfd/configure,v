  no)  commonbfdlib=false ;;
  *)   { { echo "$as_me:$LINENO: error: bad value ${enableval} for BFD commonbfdlib option" >&5
echo "$as_me: error: bad value ${enableval} for BFD commonbfdlib option" >&2;}
   { (exit 1); exit 1; }; } ;;
esac
fi


if expr a : '\(a\)' >/dev/null 2>&1; then
d119 1
a119 1
if (basename /) >/dev/null 2>&1 && test "X`basename / 2>&1`" = "X/"; then
d127 1
a127 1
as_me=`$as_basename "$0" ||
d130 1
a130 2
	 X"$0" : 'X\(/\)$' \| \
	 .     : '\(.\)' 2>/dev/null ||
d132 24
a155 4
    sed '/^.*\/\([^/][^/]*\)\/*$/{ s//\1/; q; }
  	  /^X\/\(\/\/\)$/{ s//\1/; q; }
  	  /^X\/\(\/\).*/{ s//\1/; q; }
  	  s/.*/./; q'`
d157 29
d187 6
a192 7

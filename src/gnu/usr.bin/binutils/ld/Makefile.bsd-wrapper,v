1.6
log
@introduce new FSFISLIB (default yes) and set it to no to prevent things
like cvs, lynx, etc. to be built with -O1 -fno-omit-frame-pointer -g1
@
text
@# $MirOS: src/gnu/usr.bin/binutils/ld/Makefile.bsd-wrapper,v 1.5 2005/06/13 23:52:32 tg Exp $

.include "../Makefile.inc"

FSFISLIB=	No
FSFSRC=		${GNU_BINUTILS_LD}
MAN=		ld.1
CFARGS+=	--target=${GCCTARGET}
XARGS+=		YLWRAP="${GNUSYSTEM_AUX_DIR}/ylwrap" scriptdir=/usr/libdata \
		mkinstalldirs="${SHELL} ${GNUSYSTEM_AUX_DIR}/mkinstalldirs"
INST_TARGET=	INSTALL_MAN= transform= install
CLEANFILES+=	configdoc.texi deffilep.c deffilep.h ld.1 ld.info ldgram.c \
		ldgram.h ldint.info ldlex.c ldver.texi

${MAN}: do-build

.include <bsd.cfwrap.mk>
@



long long ll = 1LL; int i = 63;
int
main ()
{
long long llmax = (long long) -1;
     return ll << i | ll >> i | llmax / ll | llmax % ll;
  ;
  return 0;
}
_ACEOF
rm -f conftest.$ac_objext conftest$ac_exeext
if { (eval echo "$as_me:$LINENO: \"$ac_link\"") >&5
  (eval $ac_link) 2>conftest.er1

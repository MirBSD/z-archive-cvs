    *) HAVE_POSIX_PRINTF=0 ;;
  esac

  if test "$ac_cv_func_asprintf" = yes; then
    HAVE_ASPRINTF=1
  else
    HAVE_ASPRINTF=0
  fi

  if test "$ac_cv_func_snprintf" = yes; then
    HAVE_SNPRINTF=1

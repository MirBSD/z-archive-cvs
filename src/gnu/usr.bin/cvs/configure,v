echo "${ECHO_T}$jm_cv_func_gettimeofday_clobber" >&6; }
  if test $jm_cv_func_gettimeofday_clobber = yes; then

  case " $LIBOBJS " in
  *" gettimeofday.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS gettimeofday.$ac_objext"

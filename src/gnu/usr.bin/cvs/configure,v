{ echo "$as_me:$LINENO: result: $gl_cv_func_getcwd_null" >&5
echo "${ECHO_T}$gl_cv_func_getcwd_null" >&6; }


      { echo "$as_me:$LINENO: checking whether getcwd is declared" >&5

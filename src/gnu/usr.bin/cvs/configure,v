_ACEOF


		{ echo "$as_me:$LINENO: checking for uniquely sized long double" >&5
echo $ECHO_N "checking for uniquely sized long double... $ECHO_C" >&6; }
if test "${ccvs_cv_unique_float_type_long_double+set}" = set; then

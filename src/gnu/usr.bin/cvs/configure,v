else
  if set |grep ^ccvs_cv_unique_int_type_ \
			|grep "($ac_cv_sizeof_char)" >/dev/null ; then
			ccvs_cv_unique_int_type_char=no
		else
			ccvs_cv_unique_int_type_char=yes\($ac_cv_sizeof_char\)

/* end confdefs.h.  */
unsigned long long ull = 1ULL; int i = 63;
int
main ()
{
unsigned long long ullmax = (unsigned long long) -1;
     return ull << i | ull >> i | ullmax / ull | ullmax % ull;
  ;
  return 0;
}
_ACEOF

  case " $LIBOBJS " in
  *" fd-safer.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS fd-safer.$ac_objext"
 ;;
esac

  case " $LIBOBJS " in
  *" pipe-safer.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS pipe-safer.$ac_objext"
 ;;
esac





_ACEOF

         gl_have_mmap_anonymous=yes
fi
rm -f conftest*

    fi
    echo "$as_me:$LINENO: result: $gl_have_mmap_anonymous" >&5
echo "${ECHO_T}$gl_have_mmap_anonymous" >&6
    if test $gl_have_mmap_anonymous = yes; then


long longval () { return EOVERFLOW; }
unsigned long ulongval () { return EOVERFLOW; }
#include <stdio.h>
#include <stdlib.h>
int
main ()
{

  FILE *f = fopen ("conftest.val", "w");
  if (! f)
    exit (1);
  if ((EOVERFLOW) < 0)
    {
      long i = longval ();
      if (i != (EOVERFLOW))
	exit (1);
      fprintf (f, "%ld\n", i);
    }
  else
    {
      unsigned long i = ulongval ();
      if (i != (EOVERFLOW))
	exit (1);
      fprintf (f, "%lu\n", i);
    }
  exit (ferror (f) || fclose (f) != 0);

  ;
  return 0;
}

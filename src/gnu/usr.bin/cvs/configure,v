echo "${ECHO_T}$ac_cv_type_long_double" >&6; }
if test $ac_cv_type_long_double = yes; then
  gt_cv_c_long_double=yes
else
  gt_cv_c_long_double=no
fi

  if test x"$gt_cv_c_long_double" = x"yes"; then

cat >>confdefs.h <<\_ACEOF

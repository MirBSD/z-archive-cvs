    CPPFLAGS="-I$with_external_zlib/include"
  else
    CPPFLAGS="$CPPFLAGS -I$with_external_zlib/include"
  fi
  if test -z "$LDFLAGS"; then
    LDFLAGS="-L$with_external_zlib/lib"
  else
    LDFLAGS="$LDFLAGS -L$with_external_zlib/lib"
  fi
fi


	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(getdate_CPPFLAGS) $(CPPFLAGS) $(AM_CFLAGS) $(CFLAGS) -c -o getdate-allocsa.o `test -f 'allocsa.c' || echo '$(srcdir)/'`allocsa.c


	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(getdate_CPPFLAGS) $(CPPFLAGS) $(AM_CFLAGS) $(CFLAGS) -c -o getdate-allocsa.obj `if test -f 'allocsa.c'; then $(CYGPATH_W) 'allocsa.c'; else $(CYGPATH_W) '$(srcdir)/allocsa.c'; fi`


[
  AC_LIBSOURCES([mempcpy.c, mempcpy.h])

  dnl Persuade glibc <string.h> to declare mempcpy().
  AC_REQUIRE([AC_GNU_SOURCE])

  AC_REPLACE_FUNCS(mempcpy)
  if test $ac_cv_func_mempcpy = no; then
    gl_PREREQ_MEMPCPY
  fi
])


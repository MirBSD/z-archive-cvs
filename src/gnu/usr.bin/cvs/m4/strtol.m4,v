AC_DEFUN([gl_FUNC_STRTOL],
[
  AC_REPLACE_FUNCS(strtol)
  if test $ac_cv_func_strtol = no; then
    gl_PREREQ_STRTOL
  fi
])

# Prerequisites of lib/strtol.c.
AC_DEFUN([gl_PREREQ_STRTOL], [
  AC_REQUIRE([AC_HEADER_STDC])
  AC_CHECK_FUNCS_ONCE(isascii)
])
@



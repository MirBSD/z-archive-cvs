AC_DEFUN([gl_FUNC_STRSTR],
[
  AC_REPLACE_FUNCS(strstr)
  if test $ac_cv_func_strstr = no; then
    gl_PREREQ_STRSTR
  fi
])

# Prerequisites of lib/strstr.c.
AC_DEFUN([gl_PREREQ_STRSTR], [:])

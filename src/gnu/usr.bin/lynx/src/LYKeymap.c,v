const char *lec_to_lecname(int code)
{
    struct emap *mp;
    int i;
    const char *result = 0;

    for (i = 0, mp = ekmap; (*mp).name != NULL; mp++, i++) {
	if ((*mp).code == code) {
	    result = (*mp).name;
	    break;
	}
    }
    return result;

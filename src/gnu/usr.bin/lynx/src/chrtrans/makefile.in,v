iso01_uni.h:		$(srcdir)/iso01_uni.tbl		$(MAKEUCTB)
iso02_uni.h:		$(srcdir)/iso02_uni.tbl		$(MAKEUCTB)
iso03_uni.h:		$(srcdir)/iso03_uni.tbl		$(MAKEUCTB)

s/\Ga(?{push @@res, $_, $`})/xx/;
print "not " unless "$_ @@res" eq 'axxa aaa a';
print "ok 4\n";

$_ = $t;
@@res = ();
pos = 1;
s/\Ga(?{push @@res, $_, $`})/x/;
print "not " unless "$_ @@res" eq 'axa aaa a';
print "ok 5\n";

$a = $t;

@#!./perl

BEGIN {
    chdir 't' if -d 't';
    @@INC = '../lib';
    require Config; import Config;
}

print "1..13\n";

$_ = 'x' x 20; 
s/\d*|x/<$&>/g; 
$foo = '<>' . ('<x><>' x 20) ;
print ($_ eq $foo ? "ok 1\n" : "not ok 1\n#'$_'\n#'$foo'\n");

$t = 'aaa';

$_ = $t;
@@res = ();
pos = 1;
s/\Ga(?{push @@res, $_, $`})/xx/g;
print "not " unless "$_ @@res" eq 'axxxx aaa a aaa aa';
print "ok 2\n";

$_ = $t;
@@res = ();
pos = 1;
s/\Ga(?{push @@res, $_, $`})/x/g;

1.6
log
@we no longer provide install-reloc, libiconv works without it too,
as it comes with its own, which is recent enough, and like this,
ohlol won't warn us about GNU GPLv3 any longer
@
text
@# $MirOS: src/gnu/share/Makefile,v 1.5 2008/05/05 22:17:02 tg Exp $

.MAIN: all

all:
	@@echo "Not a normal Makefile."

.include <bsd.own.mk>
.if defined(GNUSYSTEM_AUX_DIR)
.  include "${GNUSYSTEM_AUX_DIR}/locations.mk"
.endif

.if defined(GNU_AUX_CONTRIB_DIR) && !empty(GNU_AUX_CONTRIB_DIR)

IGNORES= \
	config.libpath \
	reloc-ldflags \

.  if exists(${GNU_AUX_CONTRIB_DIR}/gnu/aux/update.mk)
.    include "${GNU_AUX_CONTRIB_DIR}/gnu/aux/update.mk"
.  endif

.else
update:
	@@echo "Error: GNU_AUX_CONTRIB_DIR not set."
.endif

.PHONY: all update
@



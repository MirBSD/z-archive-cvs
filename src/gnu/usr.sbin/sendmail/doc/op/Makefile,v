1.1
log
@Initial revision
@
text
@d1 2
a2 1
#	@@(#)Makefile	8.2 (Berkeley) 2/28/1994
d8 2
a9 9
all: op.ps

op.ps: ${SRCS}
	rm -f ${.TARGET}
	${PIC} ${SRCS} | ${EQN} | ${ROFF} > ${.TARGET}

paper.txt: ${SRCS}
	rm -f ${.TARGET}
	${PIC} ${SRCS} | ${EQN} -Tascii | ${ROFF} -Tascii > ${.TARGET}

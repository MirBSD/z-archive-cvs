typedef struct
{
	char *yesexpr;
	char *noexpr;
	char *yesstr;
	char *nostr;
} _MessagesLocale;

extern const _MessagesLocale *_CurrentMessagesLocale;
extern const _MessagesLocale  _DefaultMessagesLocale;


typedef struct
{
	char *int_curr_symbol;
	char *currency_symbol;
	char *mon_decimal_point;
	char *mon_thousands_sep;
	char *mon_grouping;
	char *positive_sign;
	char *negative_sign;
	char int_frac_digits;
	char frac_digits;
	char p_cs_precedes;
	char p_sep_by_space;
	char n_cs_precedes;
	char n_sep_by_space;
	char p_sign_posn;
	char n_sign_posn;
} _MonetaryLocale;

extern const _MonetaryLocale *_CurrentMonetaryLocale;
extern const _MonetaryLocale  _DefaultMonetaryLocale;


typedef struct
{
	const char *decimal_point;
	const char *thousands_sep;
	const char *grouping;
} _NumericLocale;

extern const _NumericLocale *_CurrentNumericLocale;

#ifndef lint
/*static char sccsid[] = "from: @@(#)cat.c	8.1 (Berkeley) 6/11/93";*/
static char rcsid[] = "$OpenBSD: cat.c,v 1.3 2003/06/02 23:28:20 millert Exp $";
#endif /* not lint */

main()
{
	register int fd;
	char c;

	fd = getfile("File", 0);
	while (read(fd, &c, 1) == 1)
		putchar(c);
	exit(0);
}

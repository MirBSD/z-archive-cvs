#include <sys/slibkern.h>

#define x_tolower(c)	(((c) >= 'A') && ((c) <= 'Z') ? (c) - 'A' + 'a' : (c))

int
strncasecmp(const char *s1, const char *s2, size_t n)
{
	while (n--) {
		if (x_tolower(*s1) != x_tolower(*s2))
			return (x_tolower(*s1) - x_tolower(*s2));
		if (*s1++ == '\0')
			break;
		s2++;
	}
	return (0);
}
@



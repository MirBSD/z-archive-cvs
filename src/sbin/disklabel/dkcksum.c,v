#include <sys/types.h>
#include <sys/disklabel.h>
#include <stdio.h>
#include "extern.h"

__SCCSID("@@(#)dkcksum.c	8.1 (Berkeley) 6/5/93");
__RCSID("$MirOS$");

u_short
dkcksum(struct disklabel *lp)
{
	u_short *start, *end;
	u_short sum = 0;

	start = (u_short *)lp;
	end = (u_short *)&lp->d_partitions[lp->d_npartitions];
	while (start < end)
		sum ^= *start++;
	return (sum);
}
@



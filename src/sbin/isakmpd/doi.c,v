#include <sys/param.h>

#include "sysdep.h"

#include "doi.h"

static
LIST_HEAD(doi_list, doi) doi_tab;

void
doi_init(void)
{
	LIST_INIT(&doi_tab);
}

struct doi *
doi_lookup(u_int8_t doi_id)
{
	struct doi     *doi;

	for (doi = LIST_FIRST(&doi_tab); doi && doi->id != doi_id;
	    doi = LIST_NEXT(doi, link));
	return doi;
}

void
doi_register(struct doi *doi)
{
	LIST_INSERT_HEAD(&doi_tab, doi, link);
}
@



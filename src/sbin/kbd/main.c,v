#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>

__RCSID("$MirOS$");

extern void kbd_list(int);
extern void kbd_set(char *, int);
static void usage(void) __dead;

extern const char *__progname;

static void
usage(void)
{
	fprintf(stderr, "usage: %s -[q]l  or  %s [-q] name\n",
	    __progname, __progname);
	exit(1);
}

int
main(int argc, char *argv[])
{
	const char *optstring = "lq";
	int ch, list_tables = 0, verbose = 1;

	while ((ch = getopt(argc, argv, optstring)) != -1)
		switch (ch) {
		case 'l':
			list_tables = 1;
			break;
		case 'q':
			verbose = 0;
			break;
		default:
			usage();
		}
	if (argc != optind + list_tables ? 0 : 1)
		usage();

	if (list_tables)
		kbd_list(verbose);
	else
		kbd_set(argv[optind], verbose);
	exit(0);
}

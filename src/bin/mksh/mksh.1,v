.de Mx
.nr cF \\n(.f
.nr cZ \\n(.s
.ds aa \&\f\\n(cF\s\\n(cZ
.if \\n(aC==0 \{\
.	if \\n(.$==0 \&MirBSD\\*(aa
.\}
.if \\n(.$==1 \{\
.	if "\\$1"."  \&MirBSD\\$1\\*(aa
.	if "\\$1","  \&MirBSD\\$1\\*(aa
.	if "\\$1"MirBSD"  \&MirBSD\\*(aa
.\}

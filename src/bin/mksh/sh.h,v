#define C_DIGIT	 BIT(1)		/* 0-9 */
#define C_LEX1	 BIT(2)		/* \t \n\0|&;<>() */
#define C_VAR1	 BIT(3)		/* *@@#!$-? */
#define C_IFSWS	 BIT(4)		/* \t \n (IFS white space) */

#define oaat1_addmem_impl(h, buf, len) do {				\
	register const uint8_t *oaat1_addmem_p = (const void *)(buf);	\
	register size_t oaat1_addmem_n = (len);				\
									\
	while (oaat1_addmem_n--) {					\
		(h) += *oaat1_addmem_p++;				\
		(h) += (h) << 10;					\
		(h) ^= (h) >> 6;					\
	}								\
d1501 7
a1507 9

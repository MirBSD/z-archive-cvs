/* NZAT/NZAAT hashes based on Bob Jenkins' one-at-a-time hash */
a1526 9
/* not zero after termination */
#define NZATFinish(h) do {					\
	if ((h) == 0)						\
		++(h);						\
	else							\
		NZAATFinish(h);					\
} while (/* CONSTCOND */ 0)

/* NULs zhlen an allen Teilen */
@



1.817
log
@batch of optimisations
@
text
@d501 1
a501 1
#define BIT(i)		(1 << (i))
d1291 1
a1291 22
#define CiCNTRL	BIT(0)	/* \x00\x1F\x7F	*/
#define CiUPPER	BIT(1)	/* AZ			*/
#define CiLOWER	BIT(2)	/* az			*/
#define CiHEXLT	BIT(3)	/* AFaf		*/
#define CiDIGIT	BIT(4)	/* 09			*/
#define CiOCTAL	BIT(5)	/* 07			*/
#define CiQCX	BIT(6)	/* #*=?[\\]`		*/
#define CiQCM	BIT(7)	/* +/:^{}~		*/
#define CiQCL	BIT(8)	/* &();<>|		*/
#define CiVAR1	BIT(9)	/* !#$*-?@@		*/
#define CiALIAS	BIT(10)	/* !%,-.@@		*/
#define CiSUB1	BIT(11)	/* +-=?			*/
#define CiSPX	BIT(12)	/* \x0B\x0C		*/
#define CiQC	BIT(13)	/* "'			*/
#define CiSUB2	BIT(14)	/* #%			*/
#define CiUNDER	BIT(15)	/* _			*/
#define CiNUL	BIT(16)	/* \x00			*/
#define CiTAB	BIT(17)	/* \x09			*/
#define CiNL	BIT(18)	/* \x0A			*/
#define CiSP	BIT(19)	/* \x20			*/

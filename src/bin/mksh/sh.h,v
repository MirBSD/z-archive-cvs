	register const uint8_t *oaat1_addstr_p = (const void *)(s);	\
	register uint8_t oaat1_addstr_c;				\
									\
	while ((oaat1_addstr_c = *oaat1_addstr_p++)) {			\
		h += oaat1_addstr_c;					\
		(h) += (h) << 10;					\
		(h) ^= (h) >> 6;					\
	}								\

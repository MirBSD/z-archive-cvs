#if HAVE_EXPSTMT
/* this macro must not evaluate its arguments several times */
#define ksh_isspace(c)	({					\
	unsigned int ksh_isspace_c = (c);			\
	(ksh_isspace_c >= 0x09 && ksh_isspace_c <= 0x0D) ||	\
	    (ksh_isspace_c == 0x20);				\
})
#else
#define ksh_isspace(c)	ksh_isspace_((unsigned int)(c))
#endif
d338 34

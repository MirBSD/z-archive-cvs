EXTERN	const char *kshname;	/* $0 */
a233 19
 * Evil hack for const correctness due to API brokenness
 */
union mksh_cchack {
	char *rw;
	const char *ro;
};
union mksh_ccphack {
	char **rw;
	const char **ro;
};
#define cstrchr(s,c) __extension__({	\
	union mksh_cchack in, out;	\
					\
	in.ro = (s);			\
	out.rw = strchr(in.rw, (c));	\
	(out.ro);			\
})


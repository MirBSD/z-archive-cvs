void afree(void *, Area *);
#define afreechk(s)	do {		\
	void *afree_t = (s);		\
	if (afree_t)			\
		afree(afree_t, ATEMP);	\
} while (0)
#define afreechv(v,s)	do {		\
	if (v)				\
		afree(s, ATEMP);	\
} while (0)

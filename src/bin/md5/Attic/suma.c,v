#include <sys/param.h>
#include <stdio.h>
#include "suma.h"

char *
SUMA_End(SUMA_CTX *ctx, char *res)
{
	SUMA_Final(ctx);

	if (res)
		snprintf(res, SUMA_DIGEST_STRING_LENGTH, "%08X", *ctx);
	else if (asprintf(&res, "%08X", *ctx) == -1)
		res = NULL;

	return (res);
}
@



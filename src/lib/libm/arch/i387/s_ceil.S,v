@d2 1
a2 1
 * Written by J.T. Conklin <jtc@@NetBSD.org>.
d8 1
a8 3
#include "abi.h"

RCSID("$NetBSD: s_ceil.S,v 1.7 2003/07/26 19:25:00 salo Exp $")
a10 3
#ifdef __i386__
	pushl	%ebp
	movl	%esp,%ebp
d13 2
a14 2
	fstcw	-4(%ebp)		/* store fpu control word */
	movw	-4(%ebp),%dx
d17 2
a18 2
	movw	%dx,-8(%ebp)
	fldcw	-8(%ebp)		/* load modfied control word */
d20 1
a20 1
	fldl	8(%ebp);		/* round */
d23 1
a23 1
	fldcw	-4(%ebp)		/* restore original control word */
d25 1
a25 15
	leave
#else
	fstcw	-12(%rsp)
	movw	-12(%rsp),%dx
	orw	$0x0800,%dx
	andw	$0xfbff,%dx
	movw	%dx,-16(%rsp)
	fldcw	-16(%rsp)
	movsd	%xmm0,-8(%rsp)
	fldl	-8(%rsp)
	frndint
	fldcw	-12(%rsp)
	fstpl	-8(%rsp)
	movsd	-8(%rsp),%xmm0
#endif
@



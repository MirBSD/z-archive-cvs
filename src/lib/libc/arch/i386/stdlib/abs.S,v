#include <machine/asm.h>

RCSID("$MirOS$")

	.intel_syntax noprefix
	.text

NENTRY(abs)
ENTRY(labs)
	mov	eax,[esp+4]
	or	eax,eax
	jns	Lpos
	neg	eax
Lpos:	ret
@



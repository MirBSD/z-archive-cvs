1.1
log
@Initial revision
@
text
@d1 20
a20 3
/*
 * Written by J.T. Conklin <jtc@@netbsd.org>.
 * Public domain.
d25 3
a27 1
#if defined(LIBC_SCCS)
a28 2
	.asciz "$OpenBSD: div.S,v 1.3 1997/07/23 20:55:20 kstailey Exp $"
#endif
d30 3
a32 3
ENTRY(div)
	movl	4(%esp),%eax
	movl	8(%esp),%ecx
d34 6
a39 4
	idiv	%ecx
	movl	%eax,4(%esp)
	movl	%edx,8(%esp)
	ret
@



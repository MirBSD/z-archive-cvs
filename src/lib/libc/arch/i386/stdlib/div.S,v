#include <machine/asm.h>

RCSID("$MirOS$")

	.intel_syntax noprefix
	.text

NENTRY(div)
ENTRY(ldiv)
	mov	eax,[esp+8]
	cdq
	idiv	dword ptr [esp+12]
	mov	ecx,[esp+4]
	mov	[ecx],eax
	mov	[ecx+4],edx
	mov	eax,ecx
	ret	4
@



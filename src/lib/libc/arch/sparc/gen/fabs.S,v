/* fabs - floating absolute value */

#include <machine/asm.h>

ENTRY(fabs)
	std	%o0, [%sp + 32]		! return value => %f0:f1
	ldd	[%sp + 32], %f0		! (via kernel %o0/%o1 slot)
	retl
	 fabss	%f0, %f0		! return absolute value
@



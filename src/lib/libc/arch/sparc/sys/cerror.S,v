#include <machine/asm.h>
#include "SYS.h"

	.globl	_C_LABEL(errno)
#ifdef PIC
FUNC(_C_LABEL(__cerror))
	ld	[%g1 + _C_LABEL(errno)], %g1
	st	%o0, [%g1]
	mov	-1, %o0
	retl
	 mov	-1, %o1
#else
FUNC(_C_LABEL(__cerror))
	sethi	%hi(_C_LABEL(errno)), %g1
	st	%o0, [%g1 + %lo(_C_LABEL(errno))]
	mov	-1, %o0
	retl
	 mov	-1, %o1
#endif
@



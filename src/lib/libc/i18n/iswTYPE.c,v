#include "mir18n.h"

__RCSID("$MirOS: src/lib/libc/i18n/iswTYPE.c,v 1.3 2007/02/02 21:06:20 tg Exp $ expanded to isw@@@@TYPE@@@@.c");

#undef isw@@@@TYPE@@@@
int
isw@@@@TYPE@@@@(wint_t wc)
{
	return (iswctype(wc, wctype_@@@@TYPE@@@@));
}

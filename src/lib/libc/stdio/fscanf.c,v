#include <stdio.h>
#include <stdarg.h>

int __svfscanf(FILE *, const char *, _BSD_VA_LIST_);

int
fscanf(FILE *fp, const char *fmt, ...)
{
	int ret;
	va_list ap;

	va_start(ap, fmt);
	ret = __svfscanf(fp, fmt, ap);
	va_end(ap);
	return (ret);
}
@



#if defined(LIBC_SCCS) && !defined(lint)
static char rcsid[] = "$OpenBSD: ferror.c,v 1.4 2004/09/28 18:12:44 otto Exp $";
#endif /* LIBC_SCCS and not lint */

#include <stdio.h>

/*
 * A subroutine version of the macro ferror.
 */
#undef ferror

int
ferror(FILE *fp)
{
	return (__sferror(fp));
}

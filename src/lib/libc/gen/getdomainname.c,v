#include <sys/param.h>
#include <sys/sysctl.h>
#include <unistd.h>

__RCSID("$MirOS: src/lib/libc/gen/getdomainname.c,v 1.2 2005/03/06 20:28:40 tg Exp $");

int
getdomainname(char *name, size_t namelen)
{
	if (namelen > 0 && name != NULL)
		*name = 0;
	return (0);
}
@



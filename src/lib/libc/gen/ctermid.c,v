#include <stdio.h>
#include <paths.h>
#include <string.h>

char *
ctermid(char *s)
{
	static char def[] = _PATH_TTY;

	if (s) {
		memmove(s, def, sizeof(_PATH_TTY));
		return(s);
	}
	return(def);
}

char *
ctermid_r(char *s)
{
	if (s)
		return ctermid(s);
	else
		return(NULL);
}

#if defined(LIBC_SCCS) && !defined(lint)
static char *rcsid = "$OpenBSD: memset.c,v 1.4 2003/06/11 21:08:16 deraadt Exp $";
#endif /* LIBC_SCCS and not lint */

#include <string.h>

void *
memset(void *dst, int c, size_t n)
{

	if (n != 0) {
		char *d = dst;

		do
			*d++ = c;
		while (--n != 0);
	}
	return (dst);
}

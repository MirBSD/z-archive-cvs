#if !defined(_KERNEL) && !defined(_STANDALONE)
#include <string.h>
__RCSID("$MirOS: src/lib/libc/string/bzero.c,v 1.5 2005/09/22 20:16:48 tg Exp $");
#else
#include <lib/libkern/libkern.h>
#endif

/*
 * bzero -- vax movc5 instruction
 */
void
bzero(void *b, size_t length)
{
	char *p;

	for (p = b; length--;)
		*p++ = '\0';
}
@



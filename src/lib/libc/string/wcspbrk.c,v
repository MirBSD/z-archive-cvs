#if defined(LIBC_SCCS) && !defined(lint)
static char *rcsid = "$OpenBSD: wcspbrk.c,v 1.1 2005/04/13 16:35:58 espie Exp $";
#endif /* LIBC_SCCS and not lint */

#include <wchar.h>

wchar_t *
wcspbrk(s, set)
	const wchar_t *s;
	const wchar_t *set;
{
	const wchar_t *p;
	const wchar_t *q;

	p = s;
	while (*p) {
		q = set;
		while (*q) {
			if (*p == *q) {
				/* LINTED interface specification */
				return (wchar_t *)p;
			}
			q++;
		}
		p++;
	}
	return NULL;
}
@



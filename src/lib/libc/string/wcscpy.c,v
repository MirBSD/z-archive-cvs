#include <wchar.h>

__warn_references(wcscpy,
    "wcscpy() is almost always misused, please use wcslcpy()");

wchar_t *
wcscpy(wchar_t *s1, const wchar_t *s2)
{
	wchar_t *p;
	const wchar_t *q;

	p = s1;
	q = s2;
	while (*q)
		*p++ = *q++;
	*p = '\0';

	return s1;
}
@



#include <stdlib.h>
#include <netdb.h>

void
freeaddrinfo(ai)
	struct addrinfo *ai;
{
	struct addrinfo *p;

	do {
		p = ai;
		ai = ai->ai_next;
		if (p->ai_canonname)
			free(p->ai_canonname);
		free((void *)p);
	} while (ai);
}
@



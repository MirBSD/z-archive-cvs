#include <sys/types.h>
#include <sys/ioctl.h>
#include <termios.h>
#include <unistd.h>

int
tcsetpgrp(int fd, pid_t pgrp)
{
	int s;

	s = pgrp;
	return (ioctl(fd, TIOCSPGRP, &s));
}

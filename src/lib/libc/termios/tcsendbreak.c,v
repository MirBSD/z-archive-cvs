#include <sys/types.h>
#include <sys/ioctl.h>
#include <sys/tty.h>
#include <sys/time.h>
#include <sys/fcntl.h>

#include <errno.h>
#include <stdio.h>
#include <termios.h>
#include <unistd.h>

__RCSID("$MirOS$");

/* ARGSUSED */
int
tcsendbreak(int fd, int len __attribute__((__unused__)))
{
	struct timeval sleepytime;

	sleepytime.tv_sec = 0;
	sleepytime.tv_usec = 400000;
	if (ioctl(fd, TIOCSBRK, 0) == -1)
		return (-1);
	(void)select(0, 0, 0, 0, &sleepytime);
	if (ioctl(fd, TIOCCBRK, 0) == -1)
		return (-1);
	return (0);
}
@



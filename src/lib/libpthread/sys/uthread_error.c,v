#ifdef _THREAD_SAFE
#include <pthread.h>
#include "pthread_private.h"

__RCSID("$MirOS$");

#undef errno
extern	int	errno;

int *
__error(void)
{
	int	*p_errno;
	if (_thread_run == _thread_initial) {
		p_errno = &errno;
	} else {
		p_errno = &_thread_run->error;
	}
	return(p_errno);
}
#endif
@



XXX changed to be const-clean; they must use -DZCONST=const in the
XXX build CPPFLAGS as well (TODO: spot all code including zlib.h)
@
text
@d1 1
a1 1
# $MirOS: src/lib/libz/Makefile,v 1.21 2007/05/07 16:16:00 tg Exp $
a52 7
includes:
.for _i in ${HDRS}
	cd ${.CURDIR} && cmp -s ${_i} ${DESTDIR}/usr/include/${_i} || \
	    ${INSTALL} ${INSTALL_COPY} -o ${BINOWN} -g ${BINGRP} \
	    -m ${NONBINMODE} ${_i} ${DESTDIR}/usr/include/
.endfor


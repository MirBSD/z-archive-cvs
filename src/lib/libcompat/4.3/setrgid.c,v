#if 0
static char *rcsid = "$OpenBSD: setrgid.c,v 1.2 2003/06/02 20:18:40 millert Exp $";
#endif /* LIBC_SCCS and not lint */

#include <sys/types.h>
#include <unistd.h>

__RCSID("$MirOS: src/lib/libcompat/4.3/setrgid.c,v 1.2 2005/04/16 19:52:23 tg Exp $");

__warn_references(setrgid, "setrgid() is deprecated, consider using setregid()");

int
setrgid(gid_t rgid)
{
	return (setregid(rgid, (gid_t)-1));
}

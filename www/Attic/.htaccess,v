1.10
log
@switch to the new website system, keeping legacy content around
@
text
@# $MirOS: www/.htaccess,v 1.9 2006/10/31 04:14:13 tg Exp $

Options ExecCGI Indexes SymLinksIfOwnerMatch
IndexOptions -FancyIndexing
AddHandler cgi-script .cgi

AddType 'text/html; charset=us-ascii' htm

# custom 404 document
ErrorDocument 404 /404.php


# forced text files
<Files LICENCE>
  ForceType text/plain
</Files>

<Files ISC-Licence>
  ForceType text/plain
</Files>

<Files MirOS-Licence>
  ForceType text/plain
</Files>

<Files gzsigkey.cer>
  ForceType application/x-x509-ca-cert
</Files>


# "hidden" files
<Files cvsweb.conf>
  Order deny,allow
  Deny from all
</Files>

<Files wiki.cfg>
  Order deny,allow
  Deny from all
</Files>

<Files wiki.map>
  Order deny,allow
  Deny from all
</Files>

<Files wikipass.sh>
  Order deny,allow
  Deny from all
</Files>
@



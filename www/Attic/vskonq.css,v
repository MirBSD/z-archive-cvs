@/* $MirOS: www/vskonq.css,v 1.8 2005/12/27 18:59:36 bsiegert Exp $ */
/*-
 * The MirOS Project - Webpages
 * Copyrighted material; read LICENCE for terms of use.
 *-
 * This is the master stylesheet for the new-style MirOS homepage.
 * Credits to Jupp cnuke Söntgen <cnuke@@66h.42h.de> for his feedback.
 */

body {
	max-width:100%;
	background-color:#444444;
	padding:2px;
}

body,p,.content {
	color:#BFBFBF;
	font-family:Times,"Times New Roman",serif;
	font-size:12pt;
}

.legalese {
	background-color:#585858;
	color:#000000;
	font-size:4pt;
	font-family:fixed,Terminal,monospace; }

.legalbig {
	background-color:#585858;
	color:#000000;
	font-size:7pt;
	font-family:fixed,Terminal,monospace; }

tt,pre {
	font-family:fixed,vga,Fixedsys,Terminal,monospace;
	font-size:14px;
}

tt {
	white-space:nowrap;
	color:#989898;
}

a {
	text-decoration:none;
	color:#ffaa00;
}

a:hover {
	text-decoration:underline;
	background-color:#333333;
	color:#ffaa00;
}

strong {
	text-decoration:underline;
}

strong:hover {
	text-decoration:none;
	color:#FFFFFF;
}

.footer {
	position:fixed;
		bottom:3px; left:3px;
		width:412px; height:16px;
	padding:7px;
	margin-bottom:3px;
	border:inset;

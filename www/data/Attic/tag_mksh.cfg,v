1.9
log
@further simplification: drop entire tag cloud functionality
@
text
@rcsid_cfg='$MirOS: www/data/tag_mksh.cfg,v 1.8 2018/05/06 13:05:16 tg Exp $'
#-
# This file is part of the MirBSD website; see LICENCE for details.

whoami=tag/mksh
pname="mksh News"
ptitle="mksh tag cloud"
cutoff=0
targets=mksh
cfghasold=1
sources=

#DEPEND data/news.inc
sources+=\ news
tags_news=mksh

#DEPEND data/wlog-10.inc
sources+=\ wlog-10
tags_wlog_10=mksh

#DEPEND data/wlog-10-tg.inc
sources+=\ wlog-10-tg
tags_wlog_10_tg=mksh

#DEPEND data/wlog2018.inc
sources+=\ wlog2018
tags_wlog2018=mksh

set -A nopermalinks -- $sources
@



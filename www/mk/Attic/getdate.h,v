#ifndef GETDATE_H
#define GETDATE_H

#include <sys/param.h>

#if HAVE_CONFIG_H
# include <config.h>
#endif

#include <stdbool.h>
#include <stdint.h>
#ifndef IN_RCS
#include "timespec.h"
#else
#include <sys/time.h>
#include <time.h>
void gettime (struct timespec *ts);
#endif

bool get_date (struct timespec *, char const *, struct timespec const *);

#endif /* GETDATE_H */
@



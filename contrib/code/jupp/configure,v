fi
rm -f conftest.sym conftest.file

fi
{ echo "$as_me:$LINENO: result: $ac_cv_func_lstat_dereferences_slashed_symlink" >&5
echo "${ECHO_T}$ac_cv_func_lstat_dereferences_slashed_symlink" >&6; }


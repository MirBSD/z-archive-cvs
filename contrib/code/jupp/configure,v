cat >>conftest.$ac_ext <<_ACEOF
/* end confdefs.h.  */

#if HAVE_UNISTD_H
#include <unistd.h>
#endif
int main() {
	/* exit succesfully if setpgrp() takes two args (*BSD systems) */
	exit(setpgrp(0, 0) != 0);
}

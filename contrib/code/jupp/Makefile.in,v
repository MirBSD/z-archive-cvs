	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(sysconf_charmapsDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(sysconf_charmapsdir)/$$f'"; \
	  $(sysconf_charmapsDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(sysconf_charmapsdir)/$$f"; \
	done


1.1.21.1
log
@Import joe-2.8 sources into vendor branch so it doesn't get lost
@
text
@d1 9
a9 4
/* Simple hash table */

#ifndef _Ihash
#define _Ihash 1
d12 1
d14 5
a18 22
#define hnext(accu,c) (((accu)<<4)+((accu)>>28)+(c))

typedef struct entry HENTRY;
struct entry
 {
 char *name;
 HENTRY *next;
 void *val;
 };

typedef struct hash HASH;
struct hash
 {
 int len;
 HENTRY **tab;
 };

unsigned long hash();
HASH *htmk();
void htrm();
void *htfind();
void *htadd();
@


